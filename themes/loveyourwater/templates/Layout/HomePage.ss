<!-- Carousel
    ================================================== -->
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
       <% include Carousel %>
    </div><!-- /.carousel -->

    <div class="container">
        <h2 class="page-header">Upcoming events</h2>
        <div class="row">
            <div class="col-sm-6">
