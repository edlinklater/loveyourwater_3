{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.2",
		"silverstripe/cms": "^3.6",
		"silverstripe/framework": "^3.6",
		"silverstripe-themes/simple": "*",
		"symbiote/silverstripe-gridfieldextensions": "*@stable",
		"benmanu/silverstripe-leafletfield": "1.0.0",
		"silverstripe/subsites": "^1.3",
		"sheadawson/silverstripe-linkable": "^1.3",
		"edgarindustries/youtubefield": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "*",
		"silverstripe/behat-extension": "*",
		"silverstripe/testsession": "*",
		"gquemener/behat-analysis-extension": "~1.0"
	},
	"config": {
		"process-timeout": 600
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
