{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.2",
		"silverstripe/cms": "3.1.*@stable",
		"silverstripe/framework": "3.1.*@stable",
		"silverstripe-themes/simple": "*",
		"titledk/silverstripe-calendar": "*",
		"silverstripe-australia/gridfieldextensions": "*@stable"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*@stable",
		"silverstripe/behat-extension": "dev-master",
		"silverstripe/testsession": "0.2.0",
		"vipsoft/jira-extension": "2.5.x-dev",
		"gquemener/behat-analysis-extension": "~1.0"
	},
	"config": {
		"process-timeout": 600	
	},
	"minimum-stability": "dev",
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:edlinklater/silverstripe-calendar.git"
		}
	]
}
